{
  "dependencies": {
    "cypress": "^12.2.0",
    "cypress-multi-reporters": "^1.6.2",
    "mocha-junit-reporter": "^2.2.0",
    "mochawesome-report-generator": "^6.2.0"
  },
  "devDependencies": {
    "@badeball/cypress-cucumber-preprocessor": "^12.2.0",
    "@bahmutov/cypress-esbuild-preprocessor": "^2.1.3",
    "@esbuild-plugins/node-modules-polyfill": "^0.1.4",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^4.2.2",
    "multiple-cucumber-html-reporter": "^3.0.2",
    "node-xlsx": "^0.21.0"
  },
  "cypress-cucumber-preprocessor": {
    "cucumberJson": {
      "generate": true,
      "outputFolder": "cypress/cucumber-json/",
      "filePrefix": "",
      "fileSuffix": ".cucumber"
    },
    "nonGlobalStepDefinitions": false,
    "stepDefinitions": "cypress/e2e/**/*.{js,ts}",
    "json": {
      "enabled": true,
      "outputFolder": "cypress/cucumber-json",
      "metadata": {
        "browser": {
          "name": "chrome",
          "version": "81"
        },
        "device": "Local test machine",
        "platform": {
          "name": "Windows",
          "version": "10"
        }
      }
    }
  },
  "scripts": {
    "test": "cypress run --config pageLoadTimeout=10000",
    "html:report": "node cucumber-html-reports.js"
  }
}
